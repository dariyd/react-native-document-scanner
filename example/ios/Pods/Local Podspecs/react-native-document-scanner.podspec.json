{
  "name": "react-native-document-scanner",
  "version": "2.0.6",
  "summary": "React Native document scanner using VisionKit (iOS) and ML Kit (Android) with support for both old and new architecture",
  "description": "React Native document scanner using VisionKit (iOS) and ML Kit (Android) with support for both old and new architecture",
  "homepage": "https://github.com/dariyd/react-native-document-scanner",
  "license": "MIT",
  "authors": {
    "Dariy": "dariy@email.com"
  },
  "platforms": {
    "ios": "13.0"
  },
  "source": {
    "git": "https://github.com/dariyd/react-native-document-scanner.git",
    "tag": "2.0.6"
  },
  "source_files": "ios/**/*.{h,c,cc,cpp,m,mm,swift}",
