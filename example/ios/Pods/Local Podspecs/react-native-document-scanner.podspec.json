    ],
    "ReactCommon/turbomodule/core": [

    ],
    "React-NativeModulesApple": [

    ],
    "Yoga": [

    ],
    "React-Fabric": [

    ],
    "React-graphics": [

    ],
    "React-utils": [

    ],
    "React-featureflags": [

    ],
    "React-debug": [

    ],
    "React-ImageManager": [

    ],
    "React-rendererdebug": [

    ],
    "React-jsi": [

    ],
    "React-renderercss": [

    ],
    "hermes-engine": [

    ],
    "React-hermes": [

    ],
    "glog": [

    ],
    "boost": [

    ],
    "DoubleConversion": [

    ],
    "fast_float": [

    ],
    "fmt": [

    ],
    "RCT-Folly": [

    ],
    "SocketRocket": [

    ],
    "RCT-Folly/Fabric": [

    ]
  },
  "pod_target_xcconfig": {
    "HEADER_SEARCH_PATHS": "\"$(PODS_ROOT)/Headers/Private/Yoga\"",
